{
  "kind": "collectionType",
  "collectionName": "study_spots",
  "info": {
    "singularName": "study-spot",
    "pluralName": "study-spots",
    "displayName": "Study_spot",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "features": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feature.feature"
    },
    "styles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::style.style"
    },
    "vibes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vibe.vibe"
    },
    "cafe_name": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "cafe_name",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "loudness": {
      "type": "integer",
      "max": 100,
      "default": 50
    },
    "study_lengths": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::study-length.study-length"
    },
    "Study_duration": {
      "type": "enumeration",
      "enum": [
        "Short",
        "Medium",
        "Long"
      ]
    },
    "show": {
      "type": "boolean"
    },
    "discount": {
      "type": "string"
    },
    "google_map_link": {
      "type": "string"
    },
    "tiktok_embed": {
      "type": "string"
    },
    "tiktok_link": {
      "type": "string"
    },
    "suburb": {
      "type": "string",
      "required": true
    },
    "Longitude": {
      "type": "float",
      "required": true
    },
    "Latitute": {
      "type": "float",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "study_spot"
    }
  }
}
